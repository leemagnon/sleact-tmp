{
    "compilerOptions": {
        "esModuleInterop": true, // import * as React from 'react' -import React from 'react'> 
        "sourceMap": true,
        "lib": ["ES2020", "DOM"],
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "Node",
        "target": "ES5",
        "strict": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": { // import A from @src/hello.js -> 절대 경로처럼 import할 수 있다.
            "@hooks/*": ["hooks/*"],
            "@components/*": ["components/*"],
            "@layouts/*": ["layouts/*"],
            "@pages/*": ["pages/*"],
            "@utils/*": ["utils/*"],
            "@typings/*": ["typings/*"],
        }
    }
}